*,
*::before,
*::after {
  box-sizing: border-box;
}

@media (prefers-reduced-motion: reduce) {
  * {
    animation: none !important;
    transition: none !important;
  }
}

body {
  font-family: "Open Sans"
,
"Helvetica Neue"
,
Arial
,
sans-serif;
  font-family: var(--btcpay-font-family-base);
  font-size: 14px;
  font-size: var(--btcpay-font-size-m);
  font-weight: 400;
  font-weight: var(--btcpay-font-weight-normal);
  line-height: 1.6;
  color: #292929;
  color: var(--btcpay-body-color);
  background-color: #f8f9fa;
  background-color: var(--btcpay-body-bg) ;
}

pre,
code {
  font-family: SFMono-Regular
,
Menlo
,
Monaco
,
Consolas
,
"Liberation Mono"
,
"Courier New"
,
monospace;
  font-family: var(--btcpay-font-family-monospace);
  border-radius: 4px;
  border-radius: var(--btcpay-border-radius);
  color: #292929;
  color: var(--btcpay-code-color);
  background-color: #dee2e6;
  background-color: var(--btcpay-code-bg) ;
}

a {
  color: #51b13e;
  color: var(--btcpay-body-color-link);
}

a:focus,
a:hover {
  color: #51b13e;
  color: var(--btcpay-body-color-link-accent);
}


/* components */

.btcpay-header {
  color: #292929;
  color: var(--btcpay-header-color);
  background-color: #ffffff;
  background-color: var(--btcpay-header-bg);
}

.btcpay-header a {
  color: #292929;
  color: var(--btcpay-header-color-link);
  text-decoration: none;
}

.btcpay-header a:focus,
  .btcpay-header a:hover {
  color: #292929;
  color: var(--btcpay-header-color-link-accent);
}

.btcpay-theme-switch {
  --btcpay-theme-switch-light-color: var(--btcpay-color-white);
  --btcpay-theme-switch-dark-color: var(--btcpay-color-neutral-900);
  --btcpay-theme-switch-icon-size: 1.25rem;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  color: #ffffff;
  color: var(--btcpay-color-white);
  background: none;
  cursor: pointer;
  height: 40px;
  width: 40px;
  border: 0;
}

.btcpay-theme-switch svg {
  height: var(--btcpay-theme-switch-icon-size);
  width: var(--btcpay-theme-switch-icon-size);
}

.btcpay-theme-switch path {
  stroke-width: .5px;
  fill: none;
}

.btcpay-theme-switch:hover .btcpay-theme-switch-light,
.btcpay-theme-switch:focus .btcpay-theme-switch-light {
  fill: var(--btcpay-theme-switch-light-color);
}

.btcpay-theme-switch:hover .btcpay-theme-switch-dark,
.btcpay-theme-switch:focus .btcpay-theme-switch-dark {
  fill: var(--btcpay-theme-switch-dark-color);
}

.btcpay-theme-switch-dark {
  display: inline-block;
  stroke: var(--btcpay-theme-switch-dark-color);
}

:root[data-btcpay-theme="dark"] .btcpay-theme-switch-dark {
  display: none;
}

@media (prefers-color-scheme: dark) {
  :root:not([data-btcpay-theme="dark"]) .btcpay-theme-switch-dark {
    display: inline-block;
  }
}

.btcpay-theme-switch-light {
  display: none;
  stroke: var(--btcpay-theme-switch-light-color);
}

:root[data-btcpay-theme="dark"] .btcpay-theme-switch-light {
  display: inline-block;
}

@media (prefers-color-scheme: dark) {
  :root:not([data-btcpay-theme="light"]) .btcpay-theme-switch-light {
    display: inline-block;
  }
}

.btcpay-footer {
  color: #ced4da;
  color: var(--btcpay-footer-color);
  background-color: #0F3B21;
  background-color: var(--btcpay-footer-bg);
}

.btcpay-footer a {
  color: #ced4da;
  color: var(--btcpay-footer-color-link);
  text-decoration: none;
}

.btcpay-footer a:focus,
  .btcpay-footer a:hover {
  color: #f8f9fa;
  color: var(--btcpay-footer-color-link-accent);
}

.btcpay-footer-columns {
  display: flex;
  flex-wrap: wrap;
  width: 100%;
}

.btcpay-footer-columns > * {
  flex: 1 1 100%;
  margin-bottom: 20px;
  margin-bottom: var(--btcpay-space-m);
}

.btcpay-footer-columns h4,
  .btcpay-footer-columns h5,
  .btcpay-footer-columns h6 {
  margin: 20px 0;
  margin: var(--btcpay-space-m) 0;
  font-weight: 700;
  font-weight: var(--btcpay-font-weight-bold);
}

.btcpay-footer-columns ul {
  margin: 0;
  padding: 0;
  list-style: none;
}

.btcpay-footer-columns ul li {
  padding: 5px 0;
  padding: var(--btcpay-space-xs) 0;
}

@media (min-width: 576px) {
  .btcpay-footer-columns > * {
    flex: 1;
  }
  .btcpay-footer-columns > * + * {
    margin-left: 20px;
    margin-left: var(--btcpay-space-m);
  }
}
